  "get": {
    "operationId": "get/remoteManagement/profiles",
    "summary": "List OpenVPN profiles",
    "description": "These profiles are used to establish a OpenVPN connection to a data center\nwhich will give you access to the IPMI interface of your dedicated server.\nAccess to this interface allows a user to remotely administer the server in\ncase it is not reachable.",
    "responses": {
      "200": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "$schema": "http://json-schema.org/draft-04/schema",
              "id": "http://jsonschema.net",
              "properties": {
                "_metadata": {
                  "type": "object",
                  "description": "Metadata about the collection",
                  "properties": {
                    "limit": {
                      "type": "integer",
                      "description": "The maximum number of results returned",
                      "default": 20
                    },
                    "offset": {
                      "type": "integer",
                      "description": "Results are returned started at the given offset",
                      "default": 0
                    },
                    "totalCount": {
                      "type": "integer",
                      "description": "The total amount of results"
                    }
                  }
                },
                "profiles": {
                  "type": "array",
                  "description": "An array of OpenVPN profiles available for download",
                  "items": {
                    "type": "object",
                    "description": "A single OpenVPN profile",
                    "properties": {
                      "datacenter": {
                        "type": "string",
                        "description": "The datacenter code this profile is for"
                      },
                      "satelliteDatacenters": {
                        "type": "array",
                        "description": "All datacenters from config"
                      },
                      "file": {
                        "type": "string",
                        "description": "The location where this OpenVPN profile can be downloaded"
                      }
                    }
                  }
                }
              }
            },
            "examples": {
              "example": {
                "value": {
                  "_metadata": {
                    "limit": 10,
                    "offset": 0,
                    "totalCount": 1
                  },
                  "profiles": [
                    {
                      "datacenter": "AMS-01",
                      "satelliteDatacenters": [
                        "AMS-10",
                        "AMS-11",
                        "AMS-12"
                      ],
                      "file": "https://api.leaseweb.com/bareMetals/v2/remoteManagement/profiles/lsw-rmvpn-AMS-01.ovpn"
                    }
                  ]
                }
              }
            }
          }
        }
