        "name": "ip",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "format": "ip"
        },
        "example": "10.0.0.1"
      },
      {
        "name": "reference",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string"
        },
        "example": "my-webserver"
      },
      {
        "name": "id",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "format": "uuid"
        },
        "example": "a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a11"
      },
      {
        "name": "contractType",
        "required": false,
        "in": "query",
        "schema": {
          "description": "Select HOURLY for billing based on hourly usage, else MONTHLY for billing per month usage",
          "type": "string",
          "enum": [
            "HOURLY",
            "MONTHLY"
          ]
        }
      },
      {
        "name": "contractState",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "DELETE_SCHEDULED"
          ]
        }
      },
      {
        "name": "imageId",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "title": "imageId",
          "description": "imageId can be either an Operating System or a UUID in case of a Custom Image",
          "example": "UBUNTU_22_04_64BIT"
        },
        "description": "Available Images can be obtained using `/v1/images`.",
        "example": "UBUNTU_22_04_64BIT"
      },
      {
        "name": "state",
        "required": false,
        "in": "query",
        "description": "The instance's current state(s), separated by commas.",
        "style": "form",
        "explode": false,
        "schema": {
          "type": "string",
          "description": "The current state",
          "enum": [
            "CREATING",
            "DESTROYED",
            "DESTROYING",
            "FAILED",
            "RUNNING",
            "STARTING",
            "STOPPED",
            "STOPPING",
            "UNKNOWN"
          ]
        }
      },
      {
