  "get": {
    "description": "List and filter load balancers",
    "operationId": "getLoadBalancerList",
    "parameters": [
      {
        "$ref": "../components/parameters/limit.json"
      },
      {
        "$ref": "../components/parameters/offset.json"
      },
      {
        "name": "ip",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "format": "ip"
        },
        "example": "10.0.0.1"
      },
      {
        "name": "reference",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string"
        },
        "example": "my-lb"
      },
      {
        "name": "id",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "format": "uuid"
        },
        "example": "a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a11"
      },
      {
        "name": "contractState",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "DELETE_SCHEDULED"
          ]
        }
      },
      {
        "name": "contractType",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "enum": [
            "HOURLY",
            "MONTHLY"
          ]
        },
        "example": "HOURLY"
      },
      {
        "name": "state",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string",
          "enum": [
            "RUNNING",
            "STARTING",
            "STOPPED",
            "STOPPING",
            "CREATING",
            "DESTROYED",
            "DESTROYING"
          ]
        },
        "example": "RUNNING"
      },
      {
        "name": "region",
        "required": false,
        "in": "query",
        "schema": {
          "title": "regionName",
          "description": "region name",
          "type": "string",
          "enum": [
            "eu-west-3",
            "us-east-1",
            "eu-central-1",
            "ap-southeast-1",
            "us-west-1",
            "eu-west-2",
            "ca-central-1"
          ]
        },
        "example": "eu-west-3",
        "description": "Available regions can be found using the List Regions endpoint."
      },
      {
        "name": "type",
        "required": false,
        "in": "query",
        "schema": {
          "type": "string"
        },
        "example": "lsw.c3.xlarge",
        "description": "Available load balancer types can be found using the List Load Balancer Types endpoint."
      }
    ],
