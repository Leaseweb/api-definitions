require 'uri'
require 'net/http'
require 'openssl'

url = URI("https://api.leaseweb.com/publicCloud/v1/autoScalingGroups?limit=SOME_INTEGER_VALUE&offset=SOME_INTEGER_VALUE&id=SOME_STRING_VALUE&instanceId=SOME_STRING_VALUE&type=SOME_STRING_VALUE&region=SOME_STRING_VALUE&reference=SOME_STRING_VALUE&state=SOME_STRING_VALUE")

http = Net::HTTP.new(url.host, url.port)
http.use_ssl = true
http.verify_mode = OpenSSL::SSL::VERIFY_NONE

request = Net::HTTP::Get.new(url)
request["X-LSW-Auth"] = 'REPLACE_KEY_VALUE'

response = http.request(request)
puts response.read_body