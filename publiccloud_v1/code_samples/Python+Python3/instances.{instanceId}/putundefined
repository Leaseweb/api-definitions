import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

payload = "{\"type\":\"lsw.m3.large\",\"reference\":\"string\",\"contractType\":\"HOURLY\",\"contractTerm\":0,\"billingFrequency\":1,\"rootDiskSize\":5}"

headers = {
    'content-type': "application/json",
    'X-LSW-Auth': "REPLACE_KEY_VALUE"
    }

conn.request("PUT", "/publicCloud/v1/instances/695ddd91-051f-4dd6-9120-938a927a47d0", payload, headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))