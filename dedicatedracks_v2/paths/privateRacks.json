  "get": {
    "operationId": "get/privateRacks",
    "summary": "List dedicated racks",
    "description": "This API returns a paginated list of all dedicated racks.\n",
    "parameters": [
      {
        "name": "limit",
        "in": "query",
        "schema": {
          "default": 10,
          "type": "integer"
        },
        "description": "Limit the number of results returned"
      },
      {
        "name": "offset",
        "in": "query",
        "schema": {
          "default": 0,
          "type": "integer"
        },
        "description": "Return results starting from the given offset"
      },
      {
        "description": "Filter the list of dedicated racks by reference.",
        "example": "my-db",
        "in": "query",
        "name": "reference",
        "schema": {
          "type": "string"
        }
      },
      {
        "schema": {
          "type": "boolean",
          "enum": [
            true,
            false
          ]
        },
        "in": "query",
        "name": "privateNetworkCapable",
        "description": "Filter the list of private network capable dedicated racks"
      },
      {
        "schema": {
          "type": "boolean",
          "enum": [
            true,
            false
          ]
        },
        "in": "query",
        "name": "privateNetworkEnabled",
        "description": "Filter the list of private network enabled dedicated racks"
      }
    ],
