import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("GET", "/cloud/v2/virtualServers/123242/credentials/%7BcredentialType%7D/%7Busername%7D", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))